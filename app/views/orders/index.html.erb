<h1>Listing Orders</h1>
 
<table>
  <tr>
    <th>Order ID</th>
    <th>Product ID</th>
    <th>Quantity</th>
    <th>Schedule</th>
    <th>Status</th>
    <th>Notes</th>
    <th>Created</th>
  </tr>
 
  <% @orders.each do |order| %>
    <tr>
      <td><%= order.id %></td>
      <td><%= order.product_id %></td>
      <td><%= order.quantity %></td>
      <td><%= order.schedule %></td>
      <td><%= order.status %></td>
      <td><%= order.notes %></td>
      <td><%= order.created_at %></td>
      <td><%= link_to 'Show', order_path(order) %></td>
      <td><%= link_to 'Edit', edit_order_path(order) %></td>
    </tr>
  <% end %>
</table>
